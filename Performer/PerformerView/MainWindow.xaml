<Window x:Class="PerformerView.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PerformerView"
        Title="Главная" Height="450" Width="800">
    <Grid>
        <Menu HorizontalAlignment="Left" MinHeight="100" Margin="25,25,25,0" MinWidth="400" VerticalAlignment="Top">
            <Image Source="Image/Logo.png" Stretch="UniformToFill" MaxHeight="100"></Image>
            <MenuItem x:Name="MenuItemProcedure" Header="Процедура" Click="MenuItemProcedure_Click"></MenuItem>
            
            <MenuItem x:Name="MenuItemVisit" Header="Посещение" Click="MenuItemVisit_Click"></MenuItem>
        </Menu>

    </Grid>
</Window>
